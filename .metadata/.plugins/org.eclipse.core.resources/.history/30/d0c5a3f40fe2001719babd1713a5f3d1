package com.example.entity;

public class User {
	
	private int id;
	private String name;
	private int age;
	private int salary;
	
	public User(){
		
	}
	public User(Long id, String name, int age, int sal){
		this.id = id;
		this.name = name;
		this.age = age;
		this .salary = sal;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public int getSalary() {
		return salary;
	}
	public void setSalary(int salary) {
		this.salary = salary;
	}
	
	

}
